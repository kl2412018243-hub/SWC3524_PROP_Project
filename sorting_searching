for (int i = 1; i < arr.length; i++) {
    int key = arr[i], j = i - 1;
    while (j >= 0 && arr[j] > key) {
        arr[j+1] = arr[j]; j--;
    }
    arr[j+1] = key;
}


int l = 0, r = arr.length - 1;
while (l <= r) {
    int m = l + (r - l) / 2;
    if (arr[m] == target) return m;
    if (arr[m] < target) l = m + 1;
    else r = m - 1;
}
return -1;




